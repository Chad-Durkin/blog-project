<div class="row">
  <div class="col-md-8">
    <h3>{{model.category.category}}</h3>
    <div class="index-blog-pane">
      {{#each model.category.games as |game|}}
        {{index-blog-tile game=game}}
      {{/each}}
    </div>
  </div>
  <div class="col-md-4">
    <div class="categories">
      {{list-category-tile categories=model.categories}}
    </div>
  </div>
</div>
